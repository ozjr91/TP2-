#include<stdio.h>
#include<stdbool.h>

bool premier_bool(int n){
	if (n<= 1 ) {
		return false;
	}
	for (int i=2 ; i<=n/2 ; i++){
		if (n%i == 0){
			return false;
		}
	}
	return true;
}

void premier(int n){
	if (premier_bool (n)){
		printf("%d est Premier .\n", n);
	}else{
		printf("%d non Premier. \n", n);
	}
}

int main(){
	int nombre;

	printf("Entrez un nombre: ");
	scanf("%d", &nombre);
	premier (nombre);

	return 0;
}
